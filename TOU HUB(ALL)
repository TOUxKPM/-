for _, scriptData in ipairs(Scripts) do
	local btn = Instance.new("TextButton")
	btn.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
	btn.Text = scriptData.ButtonName or scriptData.Name
	btn.TextColor3 = Color3.fromRGB(220, 220, 220)
	btn.Font = Enum.Font.GothamSemibold
	btn.TextSize = 16
	btn.TextWrapped = true
	btn.Parent = ScriptScroll
	
	Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 10)
	
	btn.MouseButton1Click:Connect(function()
		PopupTitleLabel.Text = scriptData.Name
		DescLabel.Text = scriptData.Description
		
		MainFrame.Visible = false
		OpenPopup()
		
		local cancelConn
		cancelConn = CancelBtn.MouseButton1Click:Connect(function()
			ClosePopup()
			MainFrame.Visible = true
			cancelConn:Disconnect()
		end)
		
		local runConn
		runConn = RunBtn.MouseButton1Click:Connect(function()
			if not RunBtn.Active then return end
			
			ClosePopup()
			MainFrame.Visible = true
			
			ShowNotify("‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏£‡∏±‡∏ô " .. scriptData.Name .. "... ‚è≥")
			
			RunBtn.Text = "‡∏£‡∏±‡∏ô‡πÅ‡∏•‡πâ‡∏ß ‚úÖ"
			RunBtn.BackgroundColor3 = Color3.fromRGB(0, 100, 0)
			RunBtn.Active = false
			
			task.spawn(function()
				local success, err = pcall(scriptData.Function)
				if success then
					if scriptData.Name == "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏´‡∏¢‡∏¥‡∏ö‡∏Ç‡∏≠‡∏á" then
						ShowNotify("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡∏´‡∏¢‡∏¥‡∏ö‡∏Ç‡∏≠‡∏á‡πÅ‡∏•‡πâ‡∏ß")
					elseif scriptData.Name == "‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÅ‡∏™‡∏á" then
						ShowNotify("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡πÄ‡∏û‡∏¥‡πà‡∏°‡πÅ‡∏™‡∏á‡πÅ‡∏•‡πâ‡∏ß")
					elseif scriptData.Name == "‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°" then
						ShowNotify("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏≠‡∏≠‡πÇ‡∏ï‡πâ‡πÉ‡∏ä‡πâ‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡πÅ‡∏•‡πâ‡∏ß")
					elseif scriptData.Name == "‡∏ß‡∏≤‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" then
						ShowNotify("‡∏ß‡∏≤‡∏õ‡∏´‡∏≤‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢‡πÅ‡∏•‡πâ‡∏ß")
					elseif scriptData.Name == "‡∏•‡πá‡∏≠‡∏Ñ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" then
						ShowNotify("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏•‡πá‡∏≠‡∏Ñ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏•‡πâ‡∏ß")
					elseif scriptData.Name == "‡∏î‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô" then
						ShowNotify("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡∏î‡∏∂‡∏á‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÅ‡∏•‡πâ‡∏ß")
					elseif scriptData.Name == "‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô" then
						ShowNotify("‡πÄ‡∏õ‡∏¥‡∏î‡∏£‡∏∞‡∏ö‡∏ö‡πÅ‡∏à‡πâ‡∏á‡πÄ‡∏ï‡∏∑‡∏≠‡∏ô‡πÄ‡∏Ç‡πâ‡∏≤-‡∏≠‡∏≠‡∏Å‡πÄ‡∏Å‡∏°‡πÅ‡∏•‡πâ‡∏ß")
