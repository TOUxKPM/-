local function switchTab(tab)
    tabHome.Visible = (tab == "home")
    tabRecommend.Visible = (tab == "recommend")
    tabThai.Visible = (tab == "thai")
    tabGlobal.Visible = (tab == "global")
    
    homeBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    homeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    recommendBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    recommendBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    thaiBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    thaiBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    globalBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    globalBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    
    if tab == "home" then
        homeBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 170)
        homeBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    elseif tab == "recommend" then
        recommendBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 170)
        recommendBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    elseif tab == "thai" then
        thaiBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 170)
        thaiBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    elseif tab == "global" then
        globalBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 170)
        globalBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
    end
end

homeBtn.MouseButton1Click:Connect(function() switchTab("home") end)
recommendBtn.MouseButton1Click:Connect(function() switchTab("recommend") end)
thaiBtn.MouseButton1Click:Connect(function() switchTab("thai") end)
globalBtn.MouseButton1Click:Connect(function() switchTab("global") end)

local toggleBtn = Instance.new("TextButton")
toggleBtn.Size = UDim2.new(0, 150, 0, 45)
toggleBtn.Position = UDim2.new(0, 20, 0.5, -22.5)
toggleBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 170)
toggleBtn.Text = "OPEN TOU HUB"
toggleBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
toggleBtn.Font = Enum.Font.SourceSansBold
toggleBtn.TextSize = 16
toggleBtn.Draggable = true
toggleBtn.Parent = gui

Instance.new("UICorner", toggleBtn).CornerRadius = UDim.new(0, 12)

local isOpen = false
toggleBtn.MouseButton1Click:Connect(function()
    isOpen = not isOpen
    main.Visible = isOpen
    toggleBtn.Text = isOpen and "CLOSE TOU HUB" or "OPEN TOU HUB"
    if isOpen then TweenService:Create(main, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {Position = UDim2.new(0.5, -260, 0.5, -210)}):Play() end
end)

local dragging = false
local dragStart = nil
local startPos = nil

top.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = main.Position
    end
end)

top.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

UIS.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        main.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

print("TOU HUB (Mixed) ‡∏£‡∏ß‡∏°‡∏ó‡∏∏‡∏Å‡∏ü‡∏µ‡πÄ‡∏à‡∏≠‡∏£‡πå‡πÄ‡∏ï‡πá‡∏°‡∏¢‡∏≤‡∏ß‡πÄ‡∏î‡∏∑‡∏≠‡∏î‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à üíÄüñ§ | ‡∏ó‡∏∏‡∏Å‡∏£‡∏∞‡∏ö‡∏ö‡∏û‡∏£‡πâ‡∏≠‡∏°‡∏£‡∏±‡∏ô‡πÇ‡∏´‡∏î ‡πÜ | ‡∏û‡∏•‡∏±‡∏á‡∏°‡∏∑‡∏î‡∏Ñ‡∏£‡∏ö‡∏ñ‡πâ‡∏ß‡∏ô‡πÅ‡∏•‡πâ‡∏ß exploiter!")
