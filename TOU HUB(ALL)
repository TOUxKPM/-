				end
			end)
			
			runConn:Disconnect()
			if cancelConn then cancelConn:Disconnect() end
		end)
	end)
end

-- ========== Tab System ==========
local currentPage = HomePage

local function ShowPage(page)
	if currentPage then currentPage.Visible = false end
	page.Visible = true
	currentPage = page
end

local function SelectTab(tabBtn)
	for _, btn in pairs(TabFrame:GetChildren()) do
		if btn:IsA("TextButton") then
			if btn == tabBtn then
				TweenService:Create(btn, tweenInfo, {BackgroundColor3 = Color3.fromRGB(0, 120, 215)}):Play()
				TweenService:Create(btn, tweenInfo, {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
			else
				TweenService:Create(btn, tweenInfo, {BackgroundColor3 = Color3.fromRGB(30, 30, 35)}):Play()
				TweenService:Create(btn, tweenInfo, {TextColor3 = Color3.fromRGB(180, 180, 180)}):Play()
			end
		end
	end
end

HomeTab.MouseButton1Click:Connect(function() ShowPage(HomePage) SelectTab(HomeTab) end)
ScriptTab.MouseButton1Click:Connect(function() ShowPage(ScriptPage) SelectTab(ScriptTab) end)
UpdateTab.MouseButton1Click:Connect(function() ShowPage(UpdatePage) SelectTab(UpdateTab) end)

ShowPage(HomePage)
SelectTab(HomeTab)

ToggleButton.MouseButton1Click:Connect(function()
	MainFrame.Visible = not MainFrame.Visible
	if MainFrame.Visible then
		ShowPage(HomePage)
		SelectTab(HomeTab)
	end
end)

