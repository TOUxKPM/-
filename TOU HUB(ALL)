local tweenInfo = TweenInfo.new(0.4, Enum.EasingStyle.Quart, Enum.EasingDirection.Out)
local fadeIn = {BackgroundTransparency = 0; Size = UDim2.new(0, 360, 0, 220)}
local fadeOut = {BackgroundTransparency = 1; Size = UDim2.new(0, 340, 0, 200)}

local function OpenPopup()
	PopupFrame.Visible = true
	PopupFrame.Size = UDim2.new(0, 340, 0, 200)
	PopupFrame.BackgroundTransparency = 1
	TweenService:Create(PopupFrame, tweenInfo, fadeIn):Play()
	RunBtn.Text = "‚ñ∂Ô∏è ‡∏£‡∏±‡∏ô"
	RunBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
	RunBtn.Active = true
end

local function ClosePopup()
	TweenService:Create(PopupFrame, tweenInfo, fadeOut):Play()
	task.delay(0.4, function()
		PopupFrame.Visible = false
	end)
end

