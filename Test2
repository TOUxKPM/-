-- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î GUI ‡∏•‡∏≠‡∏¢‡πÄ‡∏î‡∏µ‡πà‡∏¢‡∏ß
local ToggleButton = Instance.new("ImageButton")
ToggleButton.Size = UDim2.new(0, 64, 0, 64)
ToggleButton.Position = UDim2.new(0, 10, 0.5, -32)
ToggleButton.BackgroundTransparency = 1
ToggleButton.Image = "rbxassetid://91219883240615"
ToggleButton.ZIndex = 1000
ToggleButton.Parent = ToggleGui

-- ‡∏•‡∏≤‡∏Å‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î‡πÑ‡∏î‡πâ
local draggingToggle = false
local dragStartToggle, startPosToggle

ToggleButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingToggle = true
        dragStartToggle = input.Position
        startPosToggle = ToggleButton.Position
    end
end)

ToggleButton.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        draggingToggle = false
    end
end)

UIS.InputChanged:Connect(function(input)
    if draggingToggle and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - dragStartToggle
        ToggleButton.Position = UDim2.new(
            startPosToggle.X.Scale,
            startPosToggle.X.Offset + delta.X,
            startPosToggle.Y.Scale,
            startPosToggle.Y.Offset + delta.Y
        )
    end
end)

-- ‡πÄ‡∏õ‡∏¥‡∏î/‡∏õ‡∏¥‡∏î GUI ‡∏´‡∏•‡∏±‡∏Å
ToggleButton.MouseButton1Click:Connect(function()
    main.Visible = not main.Visible
end)

-- ‡∏•‡∏≤‡∏Å GUI ‡∏´‡∏•‡∏±‡∏Å
local draggingMain = false
local dragInputMain, dragStartMain, startPosMain

local function updateInputMain(input)
    local delta = input.Position - dragStartMain
    main.Position = UDim2.new(startPosMain.X.Scale, startPosMain.X.Offset + delta.X, startPosMain.Y.Scale, startPosMain.Y.Offset + delta.Y)
end

top.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        draggingMain = true
        dragStartMain = input.Position
        startPosMain = main.Position
    end
end)

top.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        dragInputMain = input
    end
end)

UIS.InputChanged:Connect(function(input)
    if draggingMain and input == dragInputMain then
        updateInputMain(input)
    end
    if input.UserInputState == Enum.UserInputState.End then
        draggingMain = false
    end
end)

